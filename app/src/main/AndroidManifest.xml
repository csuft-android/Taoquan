<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.CSUFT.taoquan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.CSUFT.taoquan.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.CSUFT.taoquan.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.CSUFT.taoquan.ui.activity.TicketActivity"
            android:launchMode="singleTop">

        </activity>
        <activity android:name="com.CSUFT.taoquan.ui.activity.ScanQrCodeActivity">

        </activity>
    </application>

</manifest>